{
  "expo": {
    "name": "photo-booth",
    "slug": "photo-booth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoSeoulBunsik.png",
    "scheme": "photobooth",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoSeoulBunsik.png",
        "backgroundColor": "#ffd300"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LogoSeoulBunsik.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#ffd300"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
